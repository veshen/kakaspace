          <view class="_li data-v-16dcf642 check-list-item">本月最热</view>
        </view>
      </view>
      <view class="_div data-v-16dcf642 center-line"></view>
      <view class="_div data-v-16dcf642 right-btn">时间倒序</view>
    </view>
    <view class="_div data-v-16dcf642 content">
      <view class="_div data-v-16dcf642 list-content">
        <view class="_div data-v-16dcf642 list-item" bindtap="handleProxy" wx:key="{item.id}" data-eventid="{{'1-'+index}}" data-comkey="{{$k}}" wx:for="{{arrData}}" wx:for-index="index" wx:for-item="item">
          <view class="_div data-v-16dcf642 like-box"> {{item.id}}
            <label class="_span data-v-16dcf642 like"></label>
          </view>
        </view>
      </view>
    </view>
    <view class="_div data-v-16dcf642 add-photo-btn" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}">
      <image class="_img data-v-16dcf642 image" src="https://resource.sa-green.cn/image/jpg/kaka%E5%8F%91%E7%8E%B0_%E7%9B%B8%E6%9C%BA.png"></image>
    </view>
    <view class="_div data-v-16dcf642 updata-photo-box" hidden="{{!(updataPhoto)}}">
      <view class="_div data-v-16dcf642 update-block">
        <view class="_div data-v-16dcf642 off-btn-box">
          <image class="_img data-v-16dcf642 off-btn" src="https://resource.sa-green.cn/image/jpg/%E5%85%B3%E9%97%AD.svg"></image>
        </view>
        <view class="_div data-v-16dcf642 update-img-list">
          <view class="_div data-v-16dcf642 update-img-item" wx:key="{index}" wx:for="{{tempFilePaths}}" wx:for-index="index" wx:for-item="item">
