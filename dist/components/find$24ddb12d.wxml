            <view class="_div data-v-16dcf642 del-btn">x</view>
          </view>
          <view class="_div data-v-16dcf642 keep-update-btn" bindtap="handleProxy" wx:if="{{tempFilePaths.length<9&&tempFilePaths.length!==0}}" data-eventid="{{'3'}}" data-comkey="{{$k}}">
            <label class="_span data-v-16dcf642 add-icon">+</label>
          </view>
        </view>
        <view class="_div data-v-16dcf642 update-file-btn-box" bindtap="handleProxy" wx:if="{{tempFilePaths.length===0}}" data-eventid="{{'4'}}" data-comkey="{{$k}}">
          <image class="_img data-v-16dcf642 update-file-img" src="https://resource.sa-green.cn/image/jpg/%E4%B8%8A%E4%BC%A0%E7%85%A7%E7%89%87.png"></image>
          <view class="_div data-v-16dcf642 text">上传照片</view>
        </view>
        <view class="_div data-v-16dcf642 photo-desc-input-box"></view>
        <view class="_div data-v-16dcf642 submit-btn">提交</view>
