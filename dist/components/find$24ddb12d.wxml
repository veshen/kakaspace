<import src="card$63db50d3" />
<template name="find$24ddb12d">
  <view class="_div data-v-16dcf642 index find-container">
    <view class="_div data-v-16dcf642 filter-box">
      <view class="_div data-v-16dcf642 left-btn active" bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}"> 本周最热
        <icon class="_icon data-v-16dcf642 down" size="10" type="success"></icon>
        <view class="_div data-v-16dcf642 line"></view>
